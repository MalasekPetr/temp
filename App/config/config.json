{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "mailbox-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mailbox/MailboxWebPart.js",
          "manifest": "./src/webparts/mailbox/MailboxWebPart.manifest.json"
        }
      ]
    },
    "mailboxesdashboard-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mailboxesdashboard/MailboxesdashboardWebPart.js",
          "manifest": "./src/webparts/mailboxesdashboard/MailboxesdashboardWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MailboxWebPartStrings": "lib/webparts/mailbox/loc/{locale}.js",
    "MailboxesdashboardWebPartStrings": "lib/webparts/mailboxesdashboard/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}